{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["ticker"],
  "properties": {
    "ticker": {
      "type": "object",
      "required": ["high", "low", "last", "buy", "sell"],
      "properties": {
        "high": {
          "type": "string"
        },
        "low": {
          "type": "string"
        },
        "last": {
          "type": "string"
        },
        "buy": {
          "type": "string"
        },
        "sell": {
          "type": "string"
        },
        "vol_btc": {
          "type": "string"
        },
        "vol_idr": {
          "type": "string"
        }
      }
    }
  }
}
